					"name": "Create user with valid token",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"\r",
									"if (pm.response.code === 401) {\r",
									"    const response = pm.response.json();\r",
									"    \r",
									"    if (response.message && response.message.includes(\"Invalid token\") ||\r",
									"        response.message && response.message.includes(\"Authentication failed\") ||\r",
									"        response.message && response.message.includes(\"Unauthorized\")) {\r",
									"        \r",
									"        pm.test(\"my_token невалиден\", () => {\r",
									"            pm.expect.fail(`Получить токен: https://gorest.co.in/consumer/login`);\r",
									"        });\r",
									"        postman.setNextRequest(null);\r",
									"    }\r",
									"}\r",
									"else {\r",
									"    pm.test(\"Status: 201 Created\", function () {\r",
									"        pm.response.to.have.status(201);\r",
									"    });\r",
									"}"
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						}
